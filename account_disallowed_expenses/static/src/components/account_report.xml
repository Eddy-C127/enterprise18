<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="account_disallowed_expenses.DisallowedExpensesReport" t-inherit="account_reports.AccountReportCustomizable" owl="1">
        <xpath expr="//div[@id='warnings_div']" position="inside">
            <div class="alert alert-info text-center mb-0 no_print" t-if="controller.options['multi_rate_in_period']">
                <span>There are multiple disallowed expenses rates in this period</span>
            </div>
        </xpath>
    </t>
</templates>

#. module: project_forecast
#: model:mail.template,body_html:project_forecast.email_template_forecast_single
msgid ""
"<div>\n"
"                    <p>Dear ${object.employee_id.name or ''},</p><br/>\n"
"                    <p>You have been assigned the following forecast.</p><br/>\n"
"                    <table style=\"table-layout: fixed;\">\n"
"                        <tr>\n"
"                            <th style=\"padding: 5px;text-align: left;\">From</th>\n"
"                            <td style=\"padding: 5px;\">${format_datetime(object.start_datetime, tz=object.employee_id.tz)}</td>\n"
"                        </tr>\n"
"                        <tr>\n"
"                            <th style=\"padding: 5px;text-align: left;\">To</th>\n"
"                            <td style=\"padding: 5px;\">${format_datetime(object.end_datetime, tz=object.employee_id.tz)}</td>\n"
"                        </tr>\n"
"                        <tr>\n"
"                            <th style=\"padding: 5px;text-align: left;\">Project</th>\n"
"                            <td style=\"padding: 5px;\">${object.project_id.name}</td>\n"
"                        </tr>\n"
"                        % if object.task_id\n"
"                            <tr>\n"
"                                <th style=\"padding: 5px;text-align: left;\">Task</th>\n"
"                                <td style=\"padding: 5px;\">${object.task_id.name or ''}</td>\n"
"                            </tr>\n"
"                        % endif\n"
"                        <tr>\n"
"                            <th style=\"padding: 5px;text-align: left;\">Planned Hours</th>\n"
"                            <td style=\"padding: 5px;\">${format_duration(object.resource_hours)}</td>\n"
"                        </tr>\n"
"\n"
"                    </table>\n"
"                    % if group_project_user_id in object.employee_id.user_id.groups_id.ids\n"
"                        <div style=\"margin: 15px; text-align: center\">\n"
"                            <a href=\"/web?#action=${ctx.get('action_id')}&amp;model=project.forecast&amp;view_type=gantt&amp;menu_id=${ctx.get('menu_id')}&amp;db=${'dbname' in ctx and ctx['dbname'] or '' }\" style=\"padding: 5px 10px; color: #FFFFFF; text-decoration: none; background-color: #875A7B; border: 1px solid #875A7B; border-radius: 3px\">View Forecast</a>\n"
"                        </div>\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""


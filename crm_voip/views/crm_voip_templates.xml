<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="crm_voip assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/crm_voip/static/src/less/crm_voip.less" type="text/less"/>
            <script type="text/javascript" src="/crm_voip/static/src/js/crm_voip.js"></script>
        </xpath>
    </template>

    <template id="crm_voip_tests" name="crm_voip tests" inherit_id="web.qunit_suite">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/crm_voip/static/src/js/crm_voip_tests.js"></script>
        </xpath>
    </template>

</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0"> <!-- todo jov -->
        <record id="account_payment_method_nacha" model="account.payment.method">
            <field name="name">NACHA</field>
            <field name="code">nacha</field>
            <field name="payment_type">outbound</field>

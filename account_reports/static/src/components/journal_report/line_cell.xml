<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="account_reports.JournalReportLineCell" t-inherit="account_reports.AccountReportLineCellCustomizable" owl="1">
        <xpath expr="//span[hasclass('acc_rep_column_value')]" position="replace">
            <t t-if="props.cell.partner_name">
                <t t-out="props.cell.name"/>
                <strong>
                    <t t-out="props.cell.partner_name"/>
                </strong>
            </t>
            <t t-else="">
                <t t-out="props.cell.name"/>
            </t>
        </xpath>
    </t>
</templates>

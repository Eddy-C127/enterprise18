<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <t t-name="sale_subscription_dashboard.salespeople_control_panel" owl="1">
        <ControlPanel display="controlPanelDisplay">
            <t t-set-slot="control-panel-top-right">
                <div class="d-flex gap-2 w-100">
                    <SalespeopleDateFilter/>
                    <SalespeopleSelector/>
                </div>
            </t>
            <t t-set-slot="control-panel-bottom-left">
                <button class="btn btn-primary" t-on-click="props.onPrintPreview">PRINT PREVIEW</button>
            </t>
        </ControlPanel>
    </t>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<templates>
    <div t-name="spreadsheet_edition.AllPivotsSidePanel" class="o_spreadsheet_pivot_side_panel">
        <div t-foreach="env.model.getters.getPivotIds()" t-as="pivotId" t-key="pivotId" t-on-click="() => this.selectPivot(pivotId)" >
            <div class="o_side_panel_select">
                <span t-esc="env.model.getters.getPivotDisplayName(pivotId)"/>
            </div>
        </div>
    </div>
</templates>

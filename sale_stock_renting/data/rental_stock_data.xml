<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="property_padding_time" model="ir.property">
            <field name="name">property_padding_time</field>
            <field name="fields_id" search="[('model','=','product.template'),('name','=','preparation_time')]"/>
            <field name="value" model="product.template" eval="obj().env.company.padding_time"/>
            <field name="type">float</field>
        </record>

        <record id="route_rental" model='stock.route'>
            <field name="name">Rental</field>
            <field name="company_id"></field>
            <field name="sequence">10</field>
            <field name="product_selectable">False</field>
            <field name="active">False</field>
        </record>
    </data>

</odoo>

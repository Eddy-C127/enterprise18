<?xml version="1.0" encoding="utf-8"?>
<odoo><data>
    <record id="survey_user_input_view_search" model="ir.ui.view">
        <field name="name">suvey.user_input.view.search.inherit.appraisal</field>
        <field name="model">survey.user_input</field>
        <field name="inherit_id" ref="survey.survey_user_input_search"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='survey_id']" position="after">
                <field name="appraisal_id"/>
            </xpath>
            <xpath expr="//filter[@name='group_by_partner']" position="after">
                <filter string="Appraisal" name="group_by_appraisal_id" domain="[]" context="{'group_by': 'appraisal_id'}"/>
            </xpath>
        </field>
    </record>

    <record id="survey_user_input_view_form" model="ir.ui.view">
        <field name="name">survey.user_input.view.form.inherit.appraisal</field>
        <field name="model">survey.user_input</field>
        <field name="inherit_id" ref="survey.survey_user_input_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='survey_id']" position="after">
                <field name="appraisal_id"/>
            </xpath>
        </field>
    </record>

</data></odoo>

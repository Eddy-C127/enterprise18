<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_view_form" model="ir.ui.view">
        <field name="name">res.company.form.inherit.l10n.f9.report</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
        <data>
            <xpath expr="//notebook" position="inside">
                <page name="l10n_cl_accounting" string="Chilean Accounting Config" >
                    <group string="Reports">
                        <field name="l10n_cl_report_tasa_ppm"/>
                    </group>
                </page>
            </xpath>
        </data>
        </field>
    </record>
</odoo>

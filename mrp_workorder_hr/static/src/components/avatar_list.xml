<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="mrp_workorder.AvatarList" t-inherit-mode="extension" owl="1">
        <t t-foreach="avatarsToDisplay" t-as="employee" t-key="employee.id">
            <t t-if="employee.id">
                <span >
                    <img t-att-src="employee.src"
                    class="rounded-circle o_avatar_connected" />
                </span>
            </t>
        </t>
        <span t-esc="stringToDiplay" readonly="1"></span>
    </t>
</templates>
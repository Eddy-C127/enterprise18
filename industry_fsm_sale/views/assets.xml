            <link rel="stylesheet" type="text/scss" href="/industry_fsm_sale/static/src/scss/fsm_product_quantity.scss"/>
            <script type="text/javascript" src="/industry_fsm_sale/static/src/js/fsm_product_quantity.js"></script>
